{
    "name": "${localWorkspaceFolderBasename}",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "python_app",
    "remoteUser": "pythonuser",
    "extensions": [
        "quarto.quarto",
        "znck.grammarly",
        "janisdd.vscode-edit-csv",
        "GitHub.copilot",
        "ms-python.python",
        "ms-toolsai.jupyter"
    ],
    "forwardPorts": [8080, 8787],
    "workspaceFolder": "/app",
    "shutdownAction": "stopCompose"
}
